            <MenuItem fx:id="mnuOpen" onAction="#handleMenuOpen" mnemonicParsing="false" text="Open" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="mnuClose" onAction="#handleMenuClose" mnemonicParsing="false" text="Close" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="mnuPref" onAction="#handleMenuPref" mnemonicParsing="false" text="Preferencesâ€¦" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="mnuQuit" onAction="#handleMenuQuit" mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <TabPane fx:id="tabControl" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
  </children>
</VBox>
